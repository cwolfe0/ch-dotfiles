Mx.yui.inst().use('node', 'yui2.datatable', 'thirdparty.strftime', 'Mx.Util', 'Mx.MenuPopup', 'Mx.Ajax', function(Y) {

    var assignments = MxData.assignmentData ? MxData.assignmentData.data : [];

    var hasAdaptiveBook = MxData.hasAdaptiveBook;
    var adaptiveIsOnline = MxData.adaptiveIsOnline;

    var adaptiveInfoButtonID = 'adaptive-info-button';
    var adaptiveInfoButtonSelector = '#' + adaptiveInfoButtonID;

    var hasDSMBook = MxData.hasDSMBook;
    var dsmEnabled = MxData.dsmEnabled;

    // Get additional Assignment data by ID (for locked/timed)
    function getAssignment(assignmentID) {
        if (!assignments) return null;
        for (var i = 0; i < assignments.length; i++) {
            if (assignments[i].assignmentID+'' == assignmentID+'') return assignments[i];
        }
        return null;
    }

    // Define column properties
    //
    var column_defs = [
        {
            key: "title",
            field: "title",
            label: "TITLE",
            className: "title",
            //resizeable: true,
            sortable: true,
            formatter: function (cell, record, column, data) {
                var rec_data = record.getData();
                var cell_node = Y.one(cell);
                var a = Y.Node.create('<a/>');
                a.setAttribute('href', rec_data.viewURI);
                a.setAttribute('id', 'title' + rec_data.assignmentID);
                a.addClass('assignmentLink'); // instrument with component/checkAssignmentAccess.js
                a.set('text', Mx.Util.decode_utf8(data));
                cell_node.appendChild(a);

                var assignmentID = record.getData('assignmentID');
                var assignment = getAssignment(assignmentID);

                // Create followup icon
                if (assignment && assignment.adaptiveType == 'followup') {
                    var followupIconNode = Y.Node.create('<div>&nbsp;</div>');
                    followupIconNode.addClass('followup');
                    followupIconNode.addClass('icon');
                    followupIconNode.set('title', 'This is an Adaptive Follow-Up assignment.');
                    Y.one(cell).append(followupIconNode);
                }
                if (assignment && assignment.dynamicModuleKey) {
                    var dsmIconNode = Y.Node.create('<div>&nbsp;</div>');
                    dsmIconNode.addClass('dsm');
                    dsmIconNode.addClass('icon');
                    dsmIconNode.set('title', 'This is a Dynamic Study Module assignment.');
                    Y.one(cell).append(dsmIconNode);
                }

                // Create password-protected "key" icon
                if (assignment && assignment.hasPass) {
                    var protectedIconNode = Y.Node.create('<div>&nbsp;</div>');
                    protectedIconNode.addClass('protected');
                    protectedIconNode.addClass('icon');
                    protectedIconNode.set('title', 'This assignment is password-protected.');
                    Y.one(cell).append(protectedIconNode);
                }

                // Create timed assignment "clock" icon if the assignment is timed for this student
                var timeLimit = parseInt(assignment.timeLimit);
                var adjustedTimeLimit = parseInt(assignment.adjustedTimeLimit);

                // Verify that this assignment has a greater-than-zero time limit. By default,
                // the adjustedTimeLimit on a timed assignment is NaN (no time adjustment
                // for this particular student)
                if (assignment && timeLimit > 0 && (isNaN(adjustedTimeLimit) || adjustedTimeLimit > 0)) {
                    var timedIconNode = Y.Node.create('<div>&nbsp;</div>');
                    timedIconNode.addClass('timed');
                    timedIconNode.addClass('icon');
                    Y.one(cell).append(timedIconNode);
                    var timeLimitDisplay = '';
                    var increasedFrom = '';

                    var adjustedTimeLimit = parseInt(assignment.adjustedTimeLimit);
                    // check if user has been given a time limit adjustment. Adjustment can be 0 (no time limit), so check for length
                    if (adjustedTimeLimit) {
                        if (adjustedTimeLimit == 0) {
                            // Typically we don't even see the icon if this is true
                            timeLimitDisplay = "Removed by Instructor";
                        }
                        else if (adjustedTimeLimit < timeLimit) {
                            // adjustment could be lower than assignment time limit if assignment time limit was increased after adjustment was given.
                            // Use assignment time limit if that is the case
                            timeLimitDisplay = timeLimit + " minutes";
                        }
                        else if (adjustedTimeLimit != timeLimit) {
                            timeLimitDisplay = adjustedTimeLimit + " minutes";
                            increasedFrom = " (increased from "+timeLimit+")";
                        }
                        else { // adjustedTimeLimit == timeLimit
                            timeLimitDisplay = timeLimit+" minutes";
                        }
                    }
                    // use assignment time limit
                    else {
                        timeLimitDisplay = timeLimit+" minutes";
                    }

                    timedIconNode.set('title', 'This assignment is a timed assignment. Time limit: ' + timeLimitDisplay + increasedFrom);
                }

                // Display message if assignment completion status has been revoked
                var isCompleteRevoked = record.getData('isCompleteRevoked') || 0;
                if (isCompleteRevoked) {
                    var completeRevokedNode = Y.Node.create('<span>Changes made. Assignment incomplete.</span>');
                    completeRevokedNode.addClass('complete-revoked');
                    Y.one(cell).append(completeRevokedNode);
                }

            }
        }
        ,
        {
            key: "isComplete",
            field: "isComplete",
            label: "COMPLETE",
            className: "isComplete",
            //resizeable: true,
            sortable: true,
            formatter: function (cell, record, column, data) {
                var isComplete = data;
                var title = Mx.Util.decode_utf8(record.getData('title'));
                var content = (isComplete == 1) ? '<span class="checkmark-icon" title="' + title + ' is complete."><i class="fa fa-check"></i></span>' : '&nbsp;';
                Y.one(cell).append(content);
            },
            sortOptions: {
                // Sort by isComplete first, then by assignment title
                sortFunction: function(a, b, desc) {
                    var comp = YAHOO.util.Sort.compare(a.getData('isComplete'), b.getData('isComplete'), desc);
                    if (comp == 0) {
                        comp = YAHOO.util.Sort.compare(a.getData('title'), b.getData('title'), false);
                    }
                    return comp;
                }
            }
        }
        ,
        {
            key: "dateDue",
            field: "dateDueArray",
            label: "DUE DATE/TIME",
            className: "dateDue",
            //resizeable: true,
            sortable: true,
            formatter: function (cell, record, column, data) {
                var date_due = data;
                var dateExtended = record.getData('dateExtended');
                var content = date_due.strftime(MxConf.dateTimeDefaultFormat) + (dateExtended == 1 ? ' *EXT*' : '');
                Y.one(cell).append(content);
            }
        }
        ,
        {
            key: 'actions',
            label: 'ACTIONS',
            className: 'actions',
            //resizeable: true,
            sortable: false,
            formatter: function (cell, record, column, data) {
                var rec_data = record.getData();
                var printAllowed = rec_data.printAllowed;
                if (printAllowed == 1) {
                    var assignmentID = rec_data.assignmentID;
                    var printViewUrl = MxData.printViewUrl+'?assignmentID='+assignmentID;
                    var a = Y.Node.create('<a/>');
                    a.setAttribute('href', printViewUrl);
                    a.setAttribute('id', 'print' + rec_data.assignmentID);
                    a.addClass('printView');
                    a.addClass('assignmentLink');
                    a.set('text', 'Print View');
                    Y.one(cell).appendChild(a);
                }
            }
        }
    ];

    var schema = {
        fields: [
            'viewURI',
            'assignmentID',
            'title',
            'isComplete',
            'isCompleteRevoked',
            {
                key: 'dateDueArray',
                parser: Mx.Util.parseDateArray
            },
            'pastDue',
            'dateExtended',
            'printAllowed',
            'protectedAssignmentID'
        ]
    };

    // The table's data source comes from the assignments array
    //
    var data_source = new YAHOO.util.LocalDataSource(assignments);
    data_source.responseSchema = schema;

    function doBeforeSortColumn(oColumn, oSortedBy ){
        var sortedBy = (oSortedBy.substr(oSortedBy.length-4) == 'desc') ? 'DESC' : 'ASC';

        var ioArgs = {
            action: 'setUserSettings',
            assignmentListSortOrder: oColumn.key + ' ' + sortedBy
        };

        Mx.Ajax.handleRequest({
            ioArgs : ioArgs
        });

        return true;
    }

    var assignmentListContainerSelector = '#assignment-list-container';

    function processEventDelegationForAssignmentLinks() {
        setupEventDelegationForAssignmentLinks(assignmentListContainerSelector);
    }

    Y.on('available', processEventDelegationForAssignmentLinks, assignmentListContainerSelector); // see checkAssignmentAccess.js

    var assignmentsTable = null;
    function makeAssignmentsTable() {
        var container = Y.one(assignmentListContainerSelector);
        if (container) {
            assignmentsTable = new YAHOO.widget.DataTable(
                Y.Node.getDOMNode(container),
                column_defs,
                data_source
            );
            var direction = (MxData.direction == 'DESC') ? YAHOO.widget.DataTable.CLASS_DESC : YAHOO.widget.DataTable.CLASS_ASC;
            assignmentsTable.sortColumn(assignmentsTable.getColumn(MxData.orderBy), direction);
            assignmentsTable.doBeforeSortColumn = doBeforeSortColumn; // save sort order as user setting
            assignmentsTable.render();
        }
    }

    Y.on('contentready', makeAssignmentsTable, '#assignment-list-container');

    function setupAdaptiveInfoButton() {
        var buttonNode = Y.one(adaptiveInfoButtonSelector);
        if (buttonNode) {

            var panelHeaderContent = '<h3><span>Adaptive Follow-Ups:</span> A <span>NEW</span> kind of assignment</h3>';

            var adaptiveLearnMoreURI = Y.one('#adaptiveLearnMoreURI')
                ? Y.one('#adaptiveLearnMoreURI').get('href') // works everywhere but IE9
                : MxData.adaptiveLearnMoreURI; // for IE9

            var panelBodyID = 'adaptive-info-panel',
                panelBodySelector = '#' + panelBodyID,
                panelBodyContent =
                    '<div id="' + panelBodyID +'" class="invisible">' +
                        '<div class="yui3-widget-bd">' +
                            '<div><div class="icon followup"></div>This icon means that an assignment is adaptive.</div>' +
                            '<div>That means it contains questions automatically selected for you to help you where you need it most. ' +
                            '<a id="adaptiveLearnMoreLink" href="' + adaptiveLearnMoreURI + '">Learn more</a></div>' +
                        '</div>' +
                '</div>';

            var panelBodyNode = Y.Node.create(panelBodyContent);
            Y.one(document.body).appendChild(panelBodyNode);

            var fnAfterInit = function() {
                Mx.Util.instrumentHelpLink(Y.one('#adaptiveLearnMoreLink'));
                Y.one(panelBodySelector).removeClass('invisible');
                buttonNode.removeClass('invisible');
            };

            Mx.MenuPopup.create(adaptiveInfoButtonSelector, panelBodySelector, {
                headerContent: panelHeaderContent,
                anchorAlign: 'BR',
                panelAlign: 'TR',
                width: '360px',
                fnAfterInit: fnAfterInit
            });
        }
    }

    hasAdaptiveBook && Y.on('contentready', setupAdaptiveInfoButton, adaptiveInfoButtonSelector);
});
