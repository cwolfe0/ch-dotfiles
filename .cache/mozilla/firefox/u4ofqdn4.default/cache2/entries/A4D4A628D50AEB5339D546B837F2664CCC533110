/**
 * This module manages a linking between an element that, when hovered over,
 * causes a YUI panel to appear next to it.  This is used as a kind of
 * tooltip or "more information" popup.  The element to hover over is called
 * the "trigger" element.
 *
 * The widget requires existing HTML to exist in a particular format:
 *
 * <div id="more-info-course-id" style="display: inline">
 *     <span class="trigger">Hover here for more info</span>
 *     <div class="popup-content" style="display: none">
 *         <div class="yui3-widget-bd">
 *             <p>More info</p>
 *         </div>
 *     </div>
 * </div>
 *
 * And the script to activate it might look like this:
 * 
 * popup = new Mx.MoreInfoPopup({
 *     contentBox: '#more-info-course-id',
 *     activate: 'hover',
 *     popdownTimeout: popdown_timeout
 * });
 * popup.render();
 *
 * The main widget element contentBox should be a div with some id.  The
 * trigger element must be a child with the class "trigger".
 * The div with class "popup-content" becomes the HTML source for a YUI
 * Popup (follow its patterns).  On render, the popup-content element will
 * be removed from the contentBox element and appended to the body of the
 * document to make it easier to position.  It's position when shown will
 * be just to the right of the trigger element (using a Panel alignment of
 * "TR", "TL" if you are familiar with the config for Panel).  The id of
 * the root panel element will be the id of this widget + '-panel' for
 * targeting by css.  In the above example, it would be
 * 'more-info-course-id-panel'.
 *
 * If the config option 'activate' is 'click', then the popup will be
 * toggle between visible and invisible with click events on the trigger.
 *
 * If the config option 'activate' is 'hover', then the popup will show
 * when the trigger element receives a mouseover, and hide on mouseout after
 * a configurable timeout (popdownTimeout), default 500 ms.  If the
 * panel is moused over while it is still visible, the panel will stay
 * showing until the next mouseout followed by the same popdown timeout.
 *
 * CONFIGURATIONS
 * left: aligns panel to the left of the element (boolean, optional)
 * belowLeft: aligns panel below and to the left of the element (boolean, optional)
 * pclass: an additional class name to be given to the generated popup panel
 * id: an externally specified identifier
 * width: integer specifying the pixel width of the panel (optional, defaults to 300)
 * leftfix: Fixes the issue where Panel pre-renders left-context items to the right,
 *   forcing horizontal scrollbars (boolean, optional)
 * zIndex: sets z-index for the popup (numeric, optional) 
 *      If zIndex is not provided the old behavior (highest zIndex at render time +1) is used.
 *
 */

Mx.yui.inst().add('Mx.MoreInfoPopup', function(Y) {

    function MoreInfoPopup(config) {
        MoreInfoPopup.superclass.constructor.apply(this, arguments);

        // Prevent the created boundingBox (default block) from forcing the whole content from being block.
        // The contentBox can be block if it wants to be, which wouldn't be affected by this style.
        this.get('boundingBox').setStyle('display', 'inline');

        config.popupContentNode.setStyle('display', 'none');
        this.set('popupContentNode', config.popupContentNode);

        this.set('triggerNode', config.triggerNode);
        this.set('popdownTimeout', config.popdownTimeout || 500);
        this.set('left', config.left);
        this.set('leftfix', config.leftfix);
        this.set('width', config.width ? (config.width + 'px') : '300px');
        this.set('belowLeft', config.belowLeft);
        this.set('pclass', config.pclass);
        this.set('id', config.id || (MoreInfoPopup.NAME + (MoreInfoPopup.COUNT++)));
        this.set('zIndex', Mx.Util.isNumber(config.zIndex) ? parseInt(config.zIndex) : 10);

        // Default activation method is 'click'
        var activate = config.activate != 'hover' ? 'click' : 'hover';
        this.set('activate', activate);
		
		MoreInfoPopup.ALL[this.get('id')] = this;
    }

    MoreInfoPopup.NAME = 'mx-moreinfopopup';

	// Management of all popups
	MoreInfoPopup.ALL = {};
	MoreInfoPopup.COUNT = 0; // for auto-id only, not a current count
	MoreInfoPopup.GET = function (id) {
		return MoreInfoPopup.ALL[id];
	};

    MoreInfoPopup.ATTRS = {
        triggerNode: {
            readOnly: true,
            writeOnce: true
        }
        ,
        popupContentNode: {
            readOnly: true,
            writeOnce: true
        }
        ,
        popupPanel: {
            readOnly: true,
            writeOnce: true
        }
        ,
        popdownTimeout: {
            readOnly: true,
            writeOnce: true
            // TODO (test): validator: function(v) { Y.Lang.isNumber(v); }
        }
        ,
        id: {
            readOnly: true,
            writeOnce: true
        }
        ,
        activate: {
            readOnly: true,
            writeOnce: true
        }
        ,
        pclass: {
            readOnly: true,
            writeOnce: true
        }
    };

    MoreInfoPopup.HTML_PARSER = {
        triggerNode: '.trigger',
        popupContentNode: '.popup-content'
    };

    Y.extend(MoreInfoPopup, Y.Widget, {
        initializer: function(config) {
        }
        ,

        renderUI: function() {
            // Move the content node from inside the widget to
            // document.body so that it can position and float well
            var popupContentNode = this.get('popupContentNode');
            popupContentNode.get('parentNode').removeChild(popupContentNode);
            Y.one(document.body).appendChild(popupContentNode);
            var triggerNode = this.get('triggerNode');
            var alignPoints = [Y.WidgetPositionAlign.TL, Y.WidgetPositionAlign.TR];
            if (this.get('left')) {
                alignPoints = [Y.WidgetPositionAlign.TR, Y.WidgetPositionAlign.TL];
            }
            else if (this.get('belowLeft')){
                alignPoints = [Y.WidgetPositionAlign.TR, Y.WidgetPositionAlign.BR];
            }
            var panel = new Y.Panel({
                id          : this.get('contentBox').getAttribute('id') + '-panel', // force the panel to have an id related to the widget container's id
                srcNode     : popupContentNode,
                width       : this.get('width'),
                zIndex      : this.get('zIndex'),
                modal       : false, // modal behavior
                render      : true,
                visible     : false, // make visible explicitly with .show()
                constrain   : true,
                hideOn      : [ {eventName: 'clickoutside'} ]
            });
            if (this.get('leftfix')) {
                panel.centered(); // In cases where invisible panel would force horizontal scrollbar
                panel.on('render', function () {
                    this.align(triggerNode, alignPoints);
                });
            }
            else {
                panel.align(triggerNode, alignPoints);
            }
            var infoPanel = this; // make 'this' available to callbacks that have a different 'this'
            var panelSelector = '#' + panel.get('id');
            Y.on('contentready', function() {
                // Distinguish the panel as one created by this widget by adding a class
                Y.one(panelSelector).addClass('yui-mx-moreinfopopup-panel');
                // Give the panel an additional identifying class, if configured
                if (infoPanel.get('pclass')) Y.one(panelSelector).addClass(infoPanel.get('pclass'));
            }, panelSelector);

            popupContentNode.setStyle('display', '');

            // Attribute for external access to the panel
            this.set('popupPanel', panel);
        }
        ,
        bindUI: function() {
            // Make 'this' available to callbacks that have a different 'this'
            var mip = this;

            if (this.get('activate') == 'click') {
                this.get('triggerNode').on('click', function(e) {
                    if (mip.get('popupPanel').get('visible')) {
                        mip.hidePopup();
                    }
                    else {
                        Mx.getOverlayManager().hideAll();
                        mip.showPopup();
                    }
                });
            }
            else {
                this.get('triggerNode').on('mouseover', function(e) {
                    if (mip.popdownTimer) {
                        mip.popdownTimer.cancel();
                        mip.popdownTimer = null;
                    }
                    Mx.getOverlayManager().hideAll();
                    mip.showPopup();
                });

                this.get('triggerNode').on('mouseout', function(e) {
                    mip.popdownTimer = Y.later(mip.get('popdownTimeout'), null, function() {
                        mip.hidePopup();
                        mip.popdownTimer = null;
                    });
                });

                this.get('popupContentNode').on('mouseover', function(e) {
                    if (mip.popdownTimer) {
                        mip.popdownTimer.cancel();
                        mip.popdownTimer = null;
                    }
                });

                this.get('popupContentNode').on('mouseout', function(e) {
                    mip.popdownTimer = Y.later(mip.get('popdownTimeout'), null, function() {
                        mip.hidePopup();
                        mip.popdownTimer = null;
                    });
                });
            }
        }
        ,

        render: function() {
            MoreInfoPopup.superclass.render.apply(this, arguments);
        }
        ,

        destroy: function() {
            MoreInfoPopup.superclass.destroy.apply(this, arguments);
            this.get('popupPanel').destroy();
        }
        ,

        showPopup: function() {
            this.get('popupPanel').show();
        }
        ,

        hidePopup: function() {
            this.get('popupPanel').hide();
        }

    });

// This could be needed if we want the widget to generate, for example,
// 'onpopup' and 'onpopdown' events.
//    Y.augment(MoreInfoPopup, Y.Event.Target);

    Mx.MoreInfoPopup = MoreInfoPopup;

});
