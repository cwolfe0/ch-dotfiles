Mx.yui.inst().use('node', 'anim', 'event-tap', 'Mx.Util', function (Y) {

    var currentAssignmentID = MxData.assignmentID,
        protectedAssignmentID = MxData.protectedAssignmentID,
        timeDisplaySetting = 'assignment' + currentAssignmentID + '_hideTime',
        hasFollowup = MxData.hasFollowup == 1 ? true : false,
        isFollowup = MxData.isFollowup == 1 ? true : false,
        useMobileUI = MxData.useMobileUI == 1 ? true : false;

    if (useMobileUI) {
        // Note: current samsung devices have trouble with tap event, but no way to discriminate samsung from other android
        // devices, so use click event for all android.
        var selectEventType = Y.UA.android ? 'click' : 'tap';

        var deselectItemRowsMobile = function() {
            Y.all('.item.selected').each(function (node) {
                node.removeClass('selected');
            });
        };
        window.deselectItemRowsMobile = deselectItemRowsMobile;

        var selectableRowsContainerSelector = isFollowup ? '.sets' : '.assignments';

        var handleAnnouncementSelect = function(e) {
            this.toggleClass('collapsed');
        };

        // Note: A click event is still fired after the tap event on touch devices.  So, block propagation of click on link
        // otherwise click will be captured higher up the delegation.
        var handleItemLinkSelect = function(e) {
            e.preventDefault();
            e.type === 'click' && e.stopPropagation();
        };

        var handleItemRowSelect = function(e) {
            deselectItemRowsMobile(); // clear previously selected row
            var linkNode = this.one('.title > a'); // will be null for non-mobile items
            var href = linkNode ? linkNode.get('href') : null;
            if (href) {
                this.addClass('selected');
                Y.later(0, null, function() { // Y.later() for safari
                    location.href = href;
                });
            }
        };

        var setupSelectableRowsMobile = function() { // set up various event delegations
            var container = Y.one(selectableRowsContainerSelector);
            isFollowup || container.delegate(selectEventType, handleAnnouncementSelect, '.text'); // announcement rows
            container.delegate(selectEventType, handleItemLinkSelect, '.item .title > a'); // item row links
            container.delegate(selectEventType, handleItemRowSelect, '.item'); // item rows
        };

        Y.on('contentready', setupSelectableRowsMobile, selectableRowsContainerSelector);
    }
    else {
        var initTimerCountDown = function() {
            if (MxData.secondsAvailable && (Y.one('#show-time') || Y.one('#countdown'))) {
                var secondsAvailable = MxData.secondsAvailable;
                var assignmentURI = MxData.limitStudentAccess ? null : MxConf.requestURI + '?assignmentID=' + currentAssignmentID;
                var showAlert = MxData.showAlert;
                Mx.Util.timerCountDown(secondsAvailable, assignmentURI, showAlert);
            }
        };
        Y.on('domready', initTimerCountDown);

        var setupStartNowLink = function() {
            if (!protectedAssignmentID) { // if protectedAssignmentID case handled by protectedAssignmentAccess.js
                var startNow = Y.one('#start-now-button');
                startNow && startNow.on('click', function() {
                    window.location = MxConf.requestURI + '?' + MxData.startNowArg + '=1&assignmentID=' + currentAssignmentID;
                });
            }
        };
        Y.on('contentready', setupStartNowLink, '#start-now-button');

        var toggleGradingPolicy = function() {
            Y.one('#grading-policy').toggleClass('invisible');
            if (Y.one('#grading-policy').hasClass('invisible')) {
                Y.all('#grading-policy-link').setAttribute('aria-expanded', 'false');
            }
            else {
                Y.all('#grading-policy-link').setAttribute('aria-expanded', 'true');
            }
        };

        var setupGradingToggleLayer = function() {
            var nodes = Y.all('#grading-policy-link, #grading-policy-close, #grading-x');
            nodes && nodes.each(function(node) {
                node.on('click', function(e) {
                    toggleGradingPolicy();
                });

                Y.on('keydown', function(e) {
                    if (e.keyCode == 13 || e.keyCode == 32) {
                        toggleGradingPolicy();
                    }
                }, node);
            });
        };
        Y.on('contentready', setupGradingToggleLayer, '#grading-policy');

        var setupShowHideTimeRemaining = function() {
            var nodes = Y.all('#show-time-remaining, #hide-time-remaining');
            nodes && nodes.each(function(node) {
                node.on('click', function(e) {
                    Y.one('#show-time').toggleClass('invisible');
                    Y.one('#countdown').toggleClass('invisible');
                    Mx.Util.saveDisplaySetting(timeDisplaySetting, e.target.get('id') == 'show-time-remaining' ? 0 : 1);
                });
            });
        };
        Y.on('contentready', setupShowHideTimeRemaining, '#notice');

        // So far just one tip for followup assignments
        var setupInfoTips = function() {
            var tipNodes = Y.all('.info-tip');
            if (tipNodes) {
                Y.use('Mx.MoreInfoPopup', function(Y) {
                    tipNodes.each(function(tipNode) {
                        if (!tipNode.hasClass('fresh')) return; // already instrumented this one
                        tipNode.removeClass('fresh');
                        var tip = new Mx.MoreInfoPopup({
                            contentBox: tipNode,
                            activate: 'click',
                            width: 330
                        });
                        tip.render();
                        Mx.getOverlayManager().register(tip.get('popupPanel'));
                    });
                });
            }
        };
        hasFollowup && Y.on('contentready', setupInfoTips, '#followup-info');

        var gradingDetailsToggleSelector = '#grading-details-toggle';

        var setupShowHideGradingDetails = function() {
            var gradingDetailsNode = Y.one('#grading-details');
            var gradingDetailsToggleNode = Y.one(gradingDetailsToggleSelector);
            var expandCollapseIcon = Y.one(gradingDetailsToggleSelector + ' > img');
            var showMoreLessLink = Y.one(gradingDetailsToggleSelector + ' > span');

            if (gradingDetailsToggleNode && gradingDetailsNode && expandCollapseIcon && showMoreLessLink) {
                gradingDetailsToggleNode.on('click', function(e) {
                    if (gradingDetailsNode.hasClass('invisible')) {
                        expandCollapseIcon.set('src', expandCollapseIcon.get('src').replace('plus', 'minus'));
                        expandCollapseIcon.set('title', expandCollapseIcon.get('title').replace('expand', 'collapse'));
                        showMoreLessLink.set('innerHTML', 'show less');
                        gradingDetailsNode.removeClass('invisible');
                    }
                    else {
                        expandCollapseIcon.set('src', expandCollapseIcon.get('src').replace('minus', 'plus'));
                        expandCollapseIcon.set('title', expandCollapseIcon.get('title').replace('collapse', 'expand'));
                        showMoreLessLink.set('innerHTML', 'show more');
                        gradingDetailsNode.addClass('invisible');
                    }
                });
                gradingDetailsToggleNode.removeClass('invisible');
            }
        };
        isFollowup && Y.on('contentready', setupShowHideGradingDetails, gradingDetailsToggleSelector);

         function setupItemScoreLinks() {
            var itemScoreLinks = Y.all('.item-score-link.invisible');
            itemScoreLinks.each(function(node) {
                node.on('click', function(e) {
                    e.preventDefault();
                    Mx.modalManager.openModalUrl({
                        title: 'Score Details',
                        contentUrl: e.target.get('href')
                    });
                });
                node.removeClass('invisible');
            });
        }
        Y.on('contentready', setupItemScoreLinks, '#assignmentView');
   }

});
